#include <stdio.h>
#include<string.h>
int main(void) {
	int i;
	char str[100],temp;
	printf("enter a string");
	scanf("%s",&str);
	printf("\n\n original string:%s",str);
	for(i=0;i<=strlen(str);i=i+1)
	{
		temp=str[i];
		str[i]=str[i+1];
		str[i+1]=temp;
		
	}
	printf("after swapping string is %s",str);
	gets();
	return 0;
}
