

#include <stdio.h>
#include <string.h>
void main()
{
  int h1,h2,m1,m2;
  printf("enter hour and minutes :");
  scanf("%d%d",&h1,&m1);
  printf("\n\ntime1:%d:%d",h1,m1);
  printf("enter hour and minutes :");
  scanf("%d%d",&h2,&m2);
  printf("\n\ntime2:%d:%d",h2,m2);
  if(h1>h2)
  {
      h1=h1-h2;
      m1=m1-m2;
  }
  else if (h1<h2)
  {
      h2=h2-h1;
      m2=m2=m1;
  
  }
  printf("\n\nthe diff of min is %d",((60 * h1)+m1));
  
  return 0;
  
}
